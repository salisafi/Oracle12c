            $(".lazy").lazyload({
                effect : "fadeIn",
                failure_limit : 10
            });
            /*$("div.lazy").lazyload({
                effect : "fadeIn"
            });*/
            $('#welcome_popup_trigger').fancybox({
                'autoDimensions': true
            });
            setTimeout(function(){
                if (!$.cookie('seenTwitterPrompt')) {
                    $('#welcome_popup_trigger').trigger('click');
                    $.cookie('seenTwitterPrompt', 'yes', { expires: 60, path: '/' });
                }
            }, 10000);
            // Hide elements not relevant to non-logged-in users
            var loggedIn = $.cookie('frontLoggedIn');
            var loggedInUserId = $.cookie('frontUserId');
            var isAdmin = $.cookie('frontUserAdmin');
            if (loggedIn == 1) {
                $.cookie('seenTwitterPrompt', 'yes', { expires: 6000, path: '/' });
                // Replacements
                $('.replace_username').html($.cookie('frontUserName'));
                $('a.replace_userlink, .replace_userlink a').each(function(){
                    var oldHref = $(this).attr('href');
                    var newHref = oldHref.replace('davechild', $.cookie('frontUserAlias'));
                    $(this).attr('href', newHref);
                });
                $('.replace_usergravatar').attr('src', $.cookie('frontUserAvatar32'));
                // Show and hide
                //$('.logged_in').show();
                $('.logged_out').hide();

                if (isAdmin) {
                    $('.admin_view').show();
                }

                $('.user_view').each(function(){
                    var userIds = $(this).attr('data-userid').split(',');
                    if ($.inArray(loggedInUserId, userIds)) {
                        $(this).show();
                    }
                });

            } else {
                $('.logged_in').hide();
                //$('.logged_out').show();
            }
            addthis.init();
        });</script> </head></html>

